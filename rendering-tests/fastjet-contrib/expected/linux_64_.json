    "recipe": {
      "about": {
        "description": "The fastjet-contrib space is intended to provide a common location for\naccess to 3rd party extensions of FastJet. The `fastjet-contrib` output\nprovides a single merged shared object library that contains all contribs,\nwhile the `fastjet-contrib-split` output provides individual shared object\nlibraries for each contrib. Please choose the output appropriate to your\nuse case and development patterns.",
        "documentation": "https://fastjet.hepforge.org/contrib/",
        "homepage": "https://fastjet.hepforge.org/contrib/",
        "license": "GPL-2.0-or-later",
        "license_family": "GPL",
        "license_file": [
          "COPYING"
        ],
        "repository": "https://github.com/fjcontrib/fjcontrib",
        "summary": "FastJet Contrib: 3rd party extensions of FastJet"
      },
      "build": {
        "number": 7,
        "script": [
          "cmake ${CMAKE_ARGS} -DCMAKE_INSTALL_PREFIX=\"${PREFIX}\" -DCMAKE_BUILD_TYPE=Release -DFASTJETCONTRIB_ENABLE_FRAGILELIB=OFF -S . -B build",
          "cmake -LH build",
          "cmake --build build --parallel=\"${CPU_COUNT}\"",
          "cmake --install build"
        ],
